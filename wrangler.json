{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "bknd",
	"main": "src/index.tsx",
	"compatibility_date": "2025-08-03",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"workers_dev": true,
	"minify": true,
	"assets": {
		"directory": "./dist"
	},
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "bknd_nov_2025_d1",
			"database_name": "bknd-nov-2025",
			"database_id": "300c8e44-d739-4096-8c6f-56ec34e0e952",
			"remote": true
		}
	],
	"r2_buckets": [
		{
			"bucket_name": "bknd-nov-2025",
			"binding": "bknd_nov_2025_r2",
			"remote": true
		}
	]
}